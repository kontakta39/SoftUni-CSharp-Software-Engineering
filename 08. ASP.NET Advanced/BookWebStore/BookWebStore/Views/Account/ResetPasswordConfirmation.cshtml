@{
    Layout = "_Layout";
}

@{
    ViewData["Title"] = "Reset password confirmation";
}

<h1>@ViewData["Title"]</h1>
<h2>Your password has been reset. Please, click the log in button and you will be redirected to the log in page.</h2>
<a id="login" asp-action="LogIn" asp-controller="Account" class="btn btn-primary">Log in</a>